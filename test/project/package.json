  "scripts": {
    "clean": "rimraf ../tmp",
    "dev": "yarn run clean && gulp develop",
    "prod": "yarn run clean && gulp build",
    "sketch": "html-sketchapp --config html-sketchapp.config.js && open -a Sketch",
    "lint": "eslint --ext .js,.vue build src",
    "precommit": "lint-staged",
    "standalone": "yarn run clean && uiengine build -s -w"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn run lint --fix",
      "git add"
    ]
  },
