{
  "private": true,
  "name": "@uiengine/sample-project",
  "description": "UIengine Sample Project",
  "license": "MIT",
  "version": "0.14.0",
  "author": "Dennis Reimann <mail@dennisreimann.de> (https://dennisreimann.de)",
  "homepage": "https://github.com/dennisreimann/uiengine",
  "bugs": "https://github.com/dennisreimann/uiengine",
  "repository": "dennisreimann/uiengine",
  "engines": {
    "node": ">=8.11"
  },
  "scripts": {
    "clean": "rimraf ../tmp",
    "setup": "yarn install && yarn run packages:link",
    "start": "yarn run clean && gulp develop",
    "build": "yarn run clean && gulp build",
    "sketch": "html-sketchapp --config html-sketchapp.config.js && open -a Sketch",
    "lint": "eslint --ext .js,.vue build src",
    "precommit": "lint-staged",
    "packages:link": "for pkg in $(ls -1FA ../../packages | grep / | tr -d /); do yarn link @uiengine/$pkg; done",
    "packages:unlink": "for pkg in $(ls -1FA ../../packages | grep / | tr -d /); do yarn unlink @uiengine/$pkg; done",
    "standalone": "yarn run clean && uiengine build -s -w"
  },
  "dependencies": {
    "ejs": "2.6.1",
    "handlebars": "4.0.11",
    "marko": "4.12.4",
    "prop-types": "15.6.2",
    "pug": "2.0.3",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "vue": "2.5.17",
    "vue-server-renderer": "2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@uiengine/adapter-ejs": "file:../../packages/adapter-ejs",
    "@uiengine/adapter-handlebars": "file:../../packages/adapter-handlebars",
    "@uiengine/adapter-html": "file:../../packages/adapter-html",
    "@uiengine/adapter-marko": "file:../../packages/adapter-marko",
    "@uiengine/adapter-pug": "file:../../packages/adapter-pug",
    "@uiengine/adapter-react": "file:../../packages/adapter-react",
    "@uiengine/adapter-vue": "file:../../packages/adapter-vue",
    "@uiengine/cli": "file:../../packages/cli",
    "@uiengine/core": "file:../../packages/core",
    "@uiengine/ui": "file:../../packages/ui",
    "@uiengine/util": "file:../../packages/util",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.2",
    "babel-preset-vue": "2.0.2",
    "browser-sync": "2.24.7",
    "connect-history-api-fallback": "1.5.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "5.5.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-standard": "4.0.0",
    "gulp": "3.9.1",
    "html-sketchapp-cli": "0.6.1",
    "lint-staged": "^7.2.2",
    "theo": "8.0.0-beta.2",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.17.2",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2",
    "webpack-stream": "5.1.1"
  }
}
