    "htmlescape": "1.1.1",
    "vue": "2.5.21",
    "vue-analytics": "5.16.1",
    "vue-meta": "1.5.6",
    "vue-router": "3.0.2",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
