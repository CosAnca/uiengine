{
  "name": "@uiengine/ui",
  "description": "UIengine user interface.",
  "license": "MIT",
  "version": "1.1.2",
  "author": "Dennis Reimann <mail@dennisreimann.de> (https://dennisreimann.de)",
  "homepage": "https://github.com/dennisreimann/uiengine",
  "bugs": "https://github.com/dennisreimann/uiengine",
  "repository": "dennisreimann/uiengine",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.11"
  },
  "main": "./src/index.js",
  "files": [
    "dist",
    "lib",
    "src/*.js"
  ],
  "keywords": [
    "uiengine"
  ],
  "scripts": {
    "clean": "rimraf dist lib",
    "start": "yarn run clean && NODE_ENV=development webpack --config build/webpack.dev.conf.js --watch",
    "build": "yarn run clean && NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "lint": "eslint --ext .js,.vue build src test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "yarn run lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@uiengine/util": "^1.1.2",
    "color": "3.0.0",
    "ejs": "2.6.1",
    "fuse.js": "3.2.1",
    "highlight.js": "9.12.0",
    "htmlescape": "1.1.1",
    "vue": "2.5.17",
    "vue-analytics": "5.16.0",
    "vue-meta": "1.5.3",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "9.1.5",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.2",
    "clipboard": "2.0.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "css-mqpacker": "7.0.0",
    "csswring": "7.0.0",
    "ejs-compiled-loader": "2.2.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-vue": "4.7.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "2.30.1",
    "iframe-resizer": "3.6.1",
    "lint-staged": "^7.2.2",
    "node-notifier": "5.2.1",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.6",
    "run-sequence": "2.2.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "url-loader": "1.1.1",
    "vue-loader": "15.4.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.17",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-merge": "4.1.4",
    "webpack-stream": "4.0.3"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
