  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.13"
  },
  "main": "./src/index.js",
  "files": [
    "dist",
    "lib",
    "src/*.js",
    "src/locales"
  ],
  "keywords": [
    "uiengine"
  ],
  "scripts": {
    "clean": "rimraf dist lib",
    "start": "yarn run clean && cross-env NODE_ENV=development webpack --watch",
